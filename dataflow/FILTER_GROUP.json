{
	"name": "FILTER_GROUP",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          emp_id as string,",
				"          Name as string,",
				"          Salary as integer,",
				"          department_name as string,",
				"          country_id as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 filter(Salary > 3000) ~> filter1",
				"filter1 aggregate(groupBy(country_id,",
				"          department_name),",
				"     Salary = sum(Salary)) ~> aggregate1",
				"aggregate1 output() ~> output1"
			]
		}
	}
}